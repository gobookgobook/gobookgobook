<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="dao.UserOrderMapper">
   <select id="getUserOrderCount" resultType="int">
      select count(*) from userorder
   </select>
   
   <select id="getUserOrderList" resultType="com.gobook.userorder.dto.UserOrderDto" parameterType="java.util.Map">
		<![CDATA[
			select * from
				(select rownum rnum, a.* from 
					(select * from userorder) a)
		]]>
	</select>
   
</mapper>